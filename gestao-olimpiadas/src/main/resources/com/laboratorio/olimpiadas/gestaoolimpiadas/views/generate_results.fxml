<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="splitpane" dividerPositions="0.8795986622073578" orientation="VERTICAL" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.laboratorio.olimpiadas.gestaoolimpiadas.controller.ResultadoController">

   <TableView fx:id="tableViewGenerateResults" maxWidth="525.0" prefHeight="523.0" prefWidth="525.0" style="-fx-background-color: #ECECEC;" stylesheets="@tableview.css" SplitPane.resizableWithParent="false" VBox.vgrow="ALWAYS">
      <columns>
         <TableColumn fx:id="colResName" prefWidth="133.0" text="Name" />
         <TableColumn fx:id="colResGenre" prefWidth="93.0" text="Genre" />
         <TableColumn fx:id="colResType" prefWidth="93.0" text="Type" />
         <TableColumn fx:id="colResScore" prefWidth="114.0" text="Score" />
         <TableColumn fx:id="colResInscriptions" prefWidth="82.0" text="Inscriptions" />
      </columns>
      <padding>
         <Insets bottom="10.0" left="10.0" top="10.0" />
      </padding>
   </TableView>

   <VBox alignment="CENTER" spacing="15.0" style="-fx-background-color: #ECECEC;" VBox.vgrow="ALWAYS">
      <Button fx:id="btnResults" style="-fx-background-color: #808080; -fx-font-weight: bold; -fx-font-size: 15px;" text="Generate Results" textFill="WHITE">
      </Button>
   </VBox>

</SplitPane>
