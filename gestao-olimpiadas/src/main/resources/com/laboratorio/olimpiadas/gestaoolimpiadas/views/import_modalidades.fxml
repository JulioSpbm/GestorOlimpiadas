<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="splitpane" dividerPositions="0.8795986622073578" orientation="VERTICAL" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">

   <TableView fx:id="tableViewImportModalidades" prefHeight="550.0" style="-fx-background-color: #ECECEC;" stylesheets="@tableview.css" SplitPane.resizableWithParent="false" VBox.vgrow="ALWAYS">
      <columns>
         <TableColumn fx:id="colModType" text="Type" />
         <TableColumn fx:id="colModName" text="Name" />
         <TableColumn fx:id="colModGenre" text="Genre" />
         <TableColumn fx:id="colModDescription" text="Description" />
         <TableColumn fx:id="colModMinParti" text="Min Participants" />
         <TableColumn fx:id="colModScore" text="Score" />
         <TableColumn fx:id="colModNumJogos" text="Num Games" />
         <TableColumn fx:id="colModRecordName" text="Record" />
         <TableColumn fx:id="colModRecordAno" text="Record" />
         <TableColumn fx:id="colModRecordRecord" text="Record" />
         <TableColumn fx:id="colModWinnerName" text="Winner" />
         <TableColumn fx:id="colModWinnerAno" text="Winner" />
         <TableColumn fx:id="colModWinnerRecord" text="Winner" />
         <TableColumn fx:id="colModRules" text="Rules" />
      </columns>
      <padding>
         <Insets bottom="10.0" left="10.0" top="10.0" />
      </padding>
   </TableView>

   <VBox alignment="CENTER" spacing="15.0" style="-fx-background-color: #ECECEC;" VBox.vgrow="ALWAYS">
      <Button fx:id="btnImportModalidades" style="-fx-background-color: #808080; -fx-font-weight: bold; -fx-font-size: 15px;" text="Import" textFill="WHITE">
      </Button>
   </VBox>

</SplitPane>
