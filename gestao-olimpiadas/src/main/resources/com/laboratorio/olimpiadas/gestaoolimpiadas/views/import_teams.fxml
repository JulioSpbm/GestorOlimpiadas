<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="splitpane" dividerPositions="0.7568922305764411" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <TableView fx:id="tableViewImportTeam" prefHeight="550.0" style="-fx-background-color: #ECECEC;" stylesheets="@tableview.css" SplitPane.resizableWithParent="false" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="colNomeTeam" prefWidth="109.0" text="Name" />
            <TableColumn fx:id="colPaisTeam" prefWidth="68.0" text="Country" />
            <TableColumn fx:id="colGeneroTeam" prefWidth="88.0" text="Gender" />
            <TableColumn fx:id="colSportTeam" prefWidth="85.0" text="Sport" />
            <TableColumn fx:id="colYearTeam" prefWidth="124.0" text="Year of Fundation" />
        </columns>
        <padding>
            <Insets bottom="10.0" left="10.0" top="10.0" />
        </padding>
    </TableView>

    <VBox alignment="CENTER" spacing="15.0" style="-fx-background-color: #ECECEC;" VBox.vgrow="ALWAYS">
        <TableView fx:id="tableViewParticipacoesTeam" maxHeight="340.0" style="-fx-background-color: #ECECEC;" stylesheets="@tableview.css" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="colAnoTeam" prefWidth="89.0" text="Year" />
                <TableColumn fx:id="colResultTeam" text="Result" />
            </columns>
            <VBox.margin>
                <Insets left="10.0" right="10.0" />
            </VBox.margin>
        </TableView>

        <Button fx:id="btnImportTeam" style="-fx-background-color: #808080; -fx-font-weight: bold; -fx-font-size: 15px;" text="Import" textFill="WHITE">
        </Button>
    </VBox>
</SplitPane>
